<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css">
    <title>All Watches</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Here's all the Watches</h1>
    <p><a href="/watches/new">Add a new watch</a></p>
      <% watches.forEach(watch => { %>
        <div class="watchCard">
          <a href="/watches/<%= watch._id %>">
          <P><%= watch.company %> <%= watch.model %></P>
          </a>
        </div>
     <% }); %>
  </body>
</html>